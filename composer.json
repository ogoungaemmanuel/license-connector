{
    "name": "xslain/license-connector",
    "description": "Connector package for License Server",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "license",
        "license-server",
        "license-manager"
    ],
    "authors": [
        {
            "name": "EgoistDeveloper",
            "email": "epsilonelittr@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/xslain/license-server/issues",
        "source": "https://github.com/xslain/license-server"
    },
    "require": {
        "php": "^8.2 || ^8.1 || ^8.0",
        "illuminate/support": "^11.0 || ^10.0 || ^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.1 || ^7.0.0",
        "phpunit/phpunit": "^10.5 || ^9.5 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Xslain\\LicenseConnector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xslain\\LicenseConnector\\LicenseConnectorServiceProvider"
            ],
            "aliases": {
                "LicenseConnector": "Xslain\\LicenseConnector\\Facades\\LicenseConnector"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
